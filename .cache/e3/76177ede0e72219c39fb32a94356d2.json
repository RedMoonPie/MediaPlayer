{"line":64,"column":8}},{"source":"Plugins/Ads/index.ts","name":null,"original":{"line":58,"column":0},"generated":{"line":64,"column":15}},{"source":"Plugins/Ads/index.ts","name":null,"original":{"line":58,"column":15},"generated":{"line":64,"column":18}},{"source":"Plugins/Ads/index.ts","name":null,"original":{"line":58,"column":0},"generated":{"line":64,"column":27}}],"sources":{"Plugins/Ads/index.ts":"import MediaPlayer from '../../assets/MediaPlayer'\n//Importamos Ads, y Ad del archivo ads.ts\nimport Ads, {Ad} from './ads'\n//clase del plugin de los ads \nclass AdsPlugin {\n    private ads:Ads;\n    private player: MediaPlayer;\n    private media: HTMLMediaElement;\n    private currentAd: Ad\n    private adsContainer: HTMLElement;\n\n    constructor(){\n        this.ads = Ads.getInstance()\n        this.adsContainer = document.createElement('div')\n        //Handler \n        this.handleTimeUpdate = this.handleTimeUpdate.bind(this)\n    }\n    //Methodo run que recibe como parametro un player \n    run(player : MediaPlayer){\n        this.player = player ;\n        this.player.container.appendChild(this.adsContainer);\n        this.media = this.player.media;\n        //tiempo entre anuncios \n        this.media.addEventListener('timeupdate', this.handleTimeUpdate);\n    }\n\n    //Handler para tiempo entre anuncios \n    private handleTimeUpdate(){\n        const currentTime = Math.floor(this.media.currentTime)\n        if (currentTime % 30 ===0){\n        this.renderAd()\n        }\n    }\n    //Renderizar el Ad Actual\n    private renderAd(){\n        if(this.currentAd){\n            return;\n        }\n        const ad = this.ads.getAd(); \n        this.currentAd = ad;\n        this.adsContainer.innerHTML = `\n        <div class = \"ads\">  \n          <a class = \"ads_link\" href = \"${this.currentAd.url} \"target=\"_blank\">\n            <img class=\"ads_img\" src=\"${this.currentAd.imageUrl}\" />\n            <div class= \"ads_info\">\n                <h5 class=\"ads_title\"> ${this.currentAd.title}</h5>\n                <p class=\"ads_body\">${this.currentAd.body}</p>\n            </div>\n           </a>\n        </div>\n        `;\n        setTimeout(() => {\n            this.currentAd = null ;\n            this.adsContainer.innerHTML = '';\n        }, 10000);\n    }\n}\nexport default AdsPlugin;"},"lineCount":null}},"error":null,"hash":"4a0926efa25a0e44222e113697ce7d94","cacheData":{"env":{}}}