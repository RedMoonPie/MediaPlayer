 this.p = true\n        this.media.play()\n\n    }\n    //metodo pause del media player\n    pause() {\n        this.media.pause()\n        this.p = false\n    }\n    //Metodo mute del media player\n    mute(){\n        this.media.muted = true ;\n    }\n}\n//Exportar el MediaPLayer\nexport default MediaPlayer;"},"lineCount":null}},"error":null,"hash":"089afcf3472e1a31d8de05f6d758be86","cacheData":{"env":{}}}